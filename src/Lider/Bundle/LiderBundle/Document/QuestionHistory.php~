<?php
namespace Lider\Bundle\LiderBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * @MongoDB\Document
 */
class QuestionHistory
{
    /**
     * @MongoDB\Id  
     */
    private $id;
    
    /**
     * @MongoDB\Int
     */
    private $playerId;
        
    /**
     * @MongoDB\Int
     */
    private $questionId;
    
    /**
     * @MongoDB\String
     */
    private $selectedAnswer;

    /**
     * @MongoDB\Boolean
     */
    private $duel;
    
    /**
     * @MongoDB\Int     
     */
    private $duelId;

    /**	
     * @MongoDB\Boolean
     */
    private $finished;
    
    /**
     * @MongoDB\Date
     */
    private $entryDate;
    
    /**
     * @MongoDB\String
     */
    private $answerOk;
    
}

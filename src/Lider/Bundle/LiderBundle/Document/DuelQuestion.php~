<?php
namespace Lider\Bundle\LiderBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * @MongoDB\Document
 */
class DuelQuestion
{
    /**
     * @MongoDB\Id  
     */
    private $id;
    
    /**
     * @MongoDB\int
     */
    private $gameId;
        
    /**
     * @MongoDB\EmbedOne(targetDocument="Question") 
     */
    private $question;

	/**
     * @MongoDB\int
     */
    private $duelId;
   
    /**	
     * @MongoDB\Timestamp 
     */
    private $starttime; 

    /** 
     * @MongoDB\Timestamp 
     */
	private $endtime;
	
	/**
     * @MongoDB\Boolean 
     */
	private $usehelp = false;
	
    /**
     * @MongoDB\EmbedOne(targetDocument="Player") 
     */     
	private $player_one;
	
    /**
     * @MongoDB\EmbedOne(targetDocument="Player") 
     */     
	private $player_two;
	
    /**
     * @MongoDB\EmbedOne(targetDocument="Answer") 
     */
	private $answer_one;
	
    /**
     * @MongoDB\EmbedOne(targetDocument="Answer") 
     */
	private $answer_two;
	
	/**
     * @MongoDB\int
     */
	private $point_one = 0;
	
	/**
     * @MongoDB\int
     */
	private $point_two = 0;
		
}
